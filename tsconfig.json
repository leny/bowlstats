{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "lib",
    "lib": ["dom","es2019"],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "paths": {
      "containers": ["containers"],
      "components": ["components"],
      "core": ["core"],
      "types": ["types"]
    },
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  },
  "include": ["src"],
  "exclude": ["node_modules", ".vscode-test"]
}
